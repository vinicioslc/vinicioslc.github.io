{
  "name": "vinicioslc.github.io",
  "private": true,
  "description": "A minimal blog template built with Gatsby",
  "version": "0.1.0",
  "author": "Vinicios Clarindo <vinicioslc@outlook.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "posts:validate": "frontmatter-validator --path ./src/content --schema ./.frontmatter-schema",
    "build": "npm run posts:validate && gatsby build",
    "develop": "npm run posts:validate && gatsby develop",
    "dev": "npm run posts:validate && npm run develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.20.6",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-manifest": "^2.3.2",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-offline": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.2",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "typeface-nunito": "1.1.3"
  },
  "devDependencies": {
    "frontmatter-validator": "^1.0.4",
    "prettier": "^2.0.2"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
